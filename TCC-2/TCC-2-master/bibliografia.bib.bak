@InProceedings{Galster2013,
  Title                    = {Exploring possibilities to analyse microblogs for dependability information in variability-intensive open source software systems},
  Author                   = {Galster, M. and Tofan, D.},
  Booktitle                = {2013 {IEEE} {International} {Symposium} on {Software} {Reliability} {Engineering} {Workshops} ({ISSREW})},
  Year                     = {2013},
  Month                    = nov,
  Pages                    = {321--325},

  Abstract                 = {Many open source software systems are variability-intensive because they are frequently adapted to different customer needs or deployment environments. Variability in these systems not only occurs in functionality, but also in quality attributes (e.g., security, reliability, dependability). Furthermore, variability in functionality and variability in quality attributes affect each other. Recently, open source communities have adopted microblogging to document and share software engineering knowledge. Microblogging dissolves boundaries between developers and other stakeholders (e.g., end users). This facilitates the involvement of many different non-technical stakeholders in the software development process. In this paper, we investigate the feasibility and suitability of utilizing microblogs to identify and analyze dependability information about open source software systems. We use the example of the currently most popular microblogging infrastructure (Twitter) and a widely used open source content management system (Drupal). Our results indicate that even though open source communities frequently share information on Twitter, challenges exist for extracting and analyzing dependability information. Furthermore, a better understanding is needed to describe the relation between issues reported by user communities in microblogs, and issues and actions documented by developer communities in change logs and release information.},
  Doi                      = {10.1109/ISSREW.2013.6688914},
  File                     = {IEEE Xplore Abstract Record:C\:\\Users\\Thiago\\AppData\\Roaming\\Mozilla\\Firefox\\Profiles\\2jvv8all.default\\zotero\\storage\\6KDQUG24\\icp.html:text/html},
  Keywords                 = {Communities, content management, dependability, dependability information, Drupal, Media, microblogging, open source content management system, Open source software, public domain software, quality attribute, reliabiltiy, Security, social networking (online), software development process, software engineering, software engineering knowledge, software quality, software reliability, Twitter, variability, variability-intensive open source software system},
  Owner                    = {Thiago},
  Timestamp                = {2015.05.05}
}

